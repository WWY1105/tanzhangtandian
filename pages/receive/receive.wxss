.reward {
  position: fixed;
  top: 460rpx;
  right: 0;
  margin-left: 20px;
  width: 102px;
  height: 93px;
  opacity:0;
  background-position: 0 -2px;
  background-repeat: no-repeat;
  background-size: 102px 1520px;
  -webkit-animation:slideInLeft 1s 1s both,hb 1s steps(15) 2s infinite both;
	animation:slideInLeft 1s 1s both,hb 1s steps(15) 2s infinite both;
}
@keyframes hb {
	0% {
	background-position:0 0
  }
  to {
    background-position:0 -1425px
  }
}
@keyframes slideInLeft {
	0% {
    opacity:0;
    -webkit-transform:translateX(-100%);
    transform:translateX(-100%)
  }
  to {
    opacity:1;
    -webkit-transform:translateX(0);
    transform:translateX(0)
  }
}



