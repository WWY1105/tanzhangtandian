.playanimat {
  width: 484rpx;
  position: absolute;
  top: 16rpx;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 99;
  margin: auto;
  height:75px;
  background-size: 484rpx 7425px;
  background-position-x: 0px;
  background-position-y: 0px;
  opacity: 1;
  background-repeat: no-repeat;
  animation:start 2s steps(51),middle 2s steps(1) 2s none,end 2s steps(50) 4s none; 
}
@keyframes start {
  0% {
    opacity: 0;
    background-position-y: 100px;
  }
  to {
    opacity: 1;
    background-position-y: -3725px;
  }
}
@keyframes middle {
  0% {
    opacity: 1;
    background-position-y: -3725px;
  }
  to {
    background-position-y: -3725px;
  }
}
@keyframes end {
  0% {
    opacity: 1;
    background-position-y: -3725px;
  }
  to {
    background-position-y: -7475px;
    opacity: 0;
  }
}

