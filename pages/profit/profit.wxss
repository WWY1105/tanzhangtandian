.header {
  height: 175rpx;
  width: 690rpx;
  padding: 0 32rpx;
  background-color: #232832;
  line-height: 175rpx;
}
.header_title {
  float: left;
  font-size: 20px;
  font-weight: 500;
  color: #ffffff;
}
.profit {
  float: right;
  font-size: 30px;
  font-weight: 500;
  color: #ffffff;
}
.yuan {
  font-size: 20px;
  font-weight: 500;
  float: right;
  line-height: 170rpx;
  color: #ffffff;
}
.content {
  width: 690rpx;
  min-height: 1058rpx;
  position: relative;
  top: -20rpx;
  padding: 0 32rpx;
  z-index: 999;
  background-color: #ffffff;
  border-top-right-radius: 15px;
  border-top-left-radius: 15px;
  overflow: hidden;
}
.leftspot {
  position: absolute;
  left: 32rpx;
  width: 10px;
  height: 10px;
  margin-top: 16px;
  background-color: #232832;
  border-radius: 50px;
}
.rightspot {
  position: absolute;
  right: 32rpx;
  width: 10px;
  height: 10px;
  margin-top: 16px;
  background-color: #232832;
  border-radius: 50px;
}
.list {
  margin-top: 72rpx;
}
.zero {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 260rpx;
  margin: auto;
  text-align: center;
  color: #999;
  width: 300rpx;
  height: 300rpx;
}
.zero image {
  width: 200rpx;
  height: 200rpx;
}
.box {
  width: 622rpx;
  height: 168rpx;
  padding: 28rpx 32rpx;
  margin-bottom: 20rpx;
  display: flex;
  box-shadow: 0 5px 10px 0 rgba(35, 40, 50, 0.15);
  background-color: #ffffff;
  flex-direction: column;
  justify-content: space-between;
}
.time {
  float: left;
  font-size: 12px;
  color: #9f9f9f;
}
.state {
  float: right;
  font-size: 14px;
  color: #ff6b1d;
}
.shopname {
  font-size: 16px;
  color: #000000;
}
.bottom {
  display: flex;
  align-items: center;
  margin-left: 28rpx;
}
.userimg {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50px;
  vertical-align: middle;
  position: relative;
  margin-left: -28rpx;
}
.uername {
  font-size: 14px;
  font-weight: 500;
  color: #232832;
  margin-left: 10px;
}
.rmb {
  font-size: 14px;
  font-weight: 500;
  color: #232832;
  position: absolute;
  right: 63rpx;
}