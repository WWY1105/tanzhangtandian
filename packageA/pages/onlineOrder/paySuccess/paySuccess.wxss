/* packageA/pages/onlineOrder/paySuccess/paySuccess.wxss */
page {
    background-image: linear-gradient(to bottom, #228bff, #f7f7f7);
    background-size: 100% 240rpx;
    background-repeat: no-repeat;
}
.contenBox{
    flex-direction: column;
    padding-top:60rpx ;
}
.bigTips{
    font-size: 64rpx;
    font-weight: bold;
    margin: 30rpx;
}
.backBtn,.seeOrder{
    width: 320rpx;
    height: 80rpx;
    border-radius: 46rpx;
    background-color: #228bff;
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
}
.seeOrder{
    background-color: #fff;
    color: #228bff;
    border: 1rpx solid #228bff;
    margin-top: 40rpx;
}
.successImg{
    width: 300rpx;
    height: 220rpx;
    margin-top: 32rpx;
    margin-bottom: 60rpx;
}